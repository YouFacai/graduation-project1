/* 下划线 */
.content-item {
  border-bottom: 0.5px solid #efeff4;
  display: flex;
  padding: 20rpx 0;
}
.content-item .imagr-course {
  position: relative;
  width: 290rpx;
  height: 160rpx;
}
.content-item .imagr-course image {
  width: 100%;
  height: 100%;
}
.content-item .imagr-course .timer-index {
  position: absolute;
  bottom: 10rpx;
  right: 10rpx;
  font-size: 12rpx;
  color: #fff;
  background-color: rgba(51, 51, 51, 0.4);
  border-radius: 20rpx;
  font-size: 22rpx;
  padding: 0 6px;
}
.content-item .right-box {
  max-width: 365rpx;
  margin-left: 20rpx;
}
.content-item .right-box .title {
  height: 88rpx;
  font-weight: 800;
  font-size: 30rpx;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  white-space: normal;
}
.content-item .right-box .teacher {
  color: #666;
}
.content-item .right-box .teacher .iconfont {
  font-size: 20rpx;
}
.content-item .right-box .teacher .text {
  font-size: 20rpx;
}
.price-mf {
  display: flex;
  font-size: 26rpx;
  justify-content: space-between;
  align-items: center;
}
.price-mf .origin {
  color: #fb7542;
}
.ren-box .iconfont {
  font-size: 20rpx;
}
.ren-box .content {
  margin: 0 20rpx;
}

